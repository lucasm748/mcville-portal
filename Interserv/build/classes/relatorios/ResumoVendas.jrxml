     estado."id" AS estado_id,
     estado."sigla" AS estado_sigla
FROM
     "public"."venda" venda INNER JOIN "public"."pessoa" pessoa ON venda."pessoa_id" = pessoa."id"
     INNER JOIN "public"."cidade" cidade ON pessoa."cidade_id" = cidade."id"
